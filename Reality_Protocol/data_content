Â Â Â Â tasks.push({ inputs: simple, response });



Â Â Â Â // messages - chat style input

Â Â Â Â let chat = {

Â Â Â Â Â Â messages: [

Â Â Â Â Â Â Â Â { role: 'system', content: 'You are a helpful assistant.' },

